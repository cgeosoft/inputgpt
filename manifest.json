{
  "manifest_version": 3,
  "name": "InputGPT",
  "description": "Fill forms using GPT",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}